pipeline{
    agent any
    tools {
  maven 'maven 1'
 
}
    stages {
  stage('git checkout') {
    steps {
      git 'https://github.com/HopeKonu/Jenkins-Full-Course-on-aws-Linux.git'
    }
  }

  stage('SonarQube QA') {
    
  steps {

   withSonarQubeEnv(installationName : 'sonarqube1' , credentialsId: 'sonar-jenkins-connection') {
    sh 'mvn -f Maven/pom.xml clean package sonar:sonar'

      
   
}
    
  }
}




}

}
